{
    "Notice: Info": {
        "prefix": "bkinfo",
        "body": [
            "this.\\$bkInfo({",
                "\ttitle: '$1',",
                "\tsubTitle: '${2}',",
                "\tshowFooter: true,",
                "\tokText: '${3:确定}',",
                "\tcancelText: '${4:取消}',",
                "\tconfirmLoading: '${5|true,false|}',",
                "\tconfirmFn: async () => {}",
                "\tcancelFn: () => {}",
            "})"
        ],
        "description": "Magic box $info"
    }
}